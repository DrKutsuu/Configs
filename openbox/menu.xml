<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://openbox.org/
                file:///usr/share/openbox/menu.xsd">

<menu id="root-menu" label="Openbox 3">
  <item label="x-terminal-emulator">
    <action name="Execute"><execute>x-terminal-emulator</execute></action>
  </item>
  <item label="nautilus">
    <action name="Execute"><execute>nautilus --no-desktop</execute></action>
  </item>
 <item label="skype">
    <action name="Execute"><execute>skype</execute></action>
  </item>
  <item label="google-chrome">
    <action name="Execute"><execute>google-chrome</execute></action>
  </item>
  s requires the presence of the 'menu' package to work -->
  <menu id="/Debian" />
  <separator />
  <menu id="client-list-menu" />
  <separator />

  <item label="obonf">
  <action name="Execute"><execute>obconf</execute></action>
  </item>
  <item label="tint2">
  <action name="Execute"><execute>tint2 &</execute></action>
  </item>


  <item label="software-center">
  <action name="Execute"><execute>software-center &</execute></action>
  </item>
  <item label="Reconfigure">
    <action name="Reconfigure" />
  </item>
  <item label="Lock">
    <action name="Execute"> <execute> gnome-screensaver-command -l </execute> </action>
  </item>
	
  <item label="Restart">
    <action name="Restart" />
  </item>
  <separator />
  
  <item label="Exit">
    <action name="Exit" />
  </item>
</menu>

</openbox_menu>
